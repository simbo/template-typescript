{
  "name": "template-typescript",
  "version": "0.1.0",
  "description": "A template repository for typescript projects.",
  "author": "Simon Lepel <simbo@simbo.de> (https://simbo.de)",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://github.com/simbo/template-typescript#readme",
  "repository": "github:simbo/template-typescript",
  "bugs": "https://github.com/simbo/template-typescript/issues",
  "private": true,
  "scripts": {
    "build": "exit 0",
    "dev": "exit 0",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:fix": "yarn run lint:eslint:fix && yarn run lint:prettier:fix",
    "lint:eslint": "eslint -c .eslintrc --ext .ts,.tsx,.js,.jsx .",
    "lint:eslint:fix": "eslint -c .eslintrc --ext .ts,.tsx,.js,jsx --fix .",
    "lint:prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,md,json,yml}\"",
    "lint:prettier:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,yml}\"",
    "preflight": "yarn run lint && yarn run test && yarn run build",
    "start": "yarn run dev",
    "test": "exit 0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-editorconfig": "git://github.com/simbo/eslint-plugin-editorconfig.git",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
